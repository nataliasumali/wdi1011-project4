	<!-- images rendered -->

	<div class="col-xs-4">

	<h1>findings:</h1>

		<div class="imagesRendered" ng-repeat="d in data['data']">

		    <img src={{d['images']['thumbnail']['url']}} class="img-thumbnail">

        <div class="caption">
          <h4><a href="" class="locationUrl" data-toggle="modal" data-target="#myModal"> {{d['location']['latitude']}},{{d['location']['longitude']}}</a></h4>
        </div>

          <!-- Modal -->
          <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog">
              <div class="modal-content">
                <div class="modal-header">
                  <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                  <h4 class="modal-title" id="myModalLabel">Modal title</h4>
                </div>
                <div class="modal-body">
                  ...
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
              </div>
            </div>
          </div>

		</div> 

